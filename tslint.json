{
    "defaultSeverity": "error",
    "exclude": [
        "**/node_modules/*"
    ],
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "variable-name": false,
        "object-literal-sort-keys": false,
        "indent": [
            true,
            "tabs"
        ],
        "semicolon": [
            true,
            "never"
        ],
        "max-line-length": false,
        "no-console": false
    },
    "rulesDirectory": []
}